
<h4><%= @musician %></h4>
<h4>Instrument</h4>

<form action="/musicians/playlists" method="post" class="form-inline">
  <button type="submit" class="btn btn-default">create playlist</button>
</form>

<div class="participants">
  <% @musicians.each do |track| %>
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
      <h4><%= track['Title'] %></h4>
      <h4><%= track['OriginalAlbumTitle'] %></h4>
      <h4><%= track['MainArtistsLiteral'] %></h4>
    </div>
  </div>
  <br>
  <% end %>
</div>
